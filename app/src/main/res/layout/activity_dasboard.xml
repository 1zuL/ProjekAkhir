<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Dasboard">

    <TextView
        android:id="@+id/tittle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="144dp"
        android:layout_marginBottom="734dp"
        android:text="Low Calory Food"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="30dp" />

    <ImageView
        android:id="@+id/keranjang"
        android:layout_width="70dp"
        android:layout_height="56dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="72dp"
        android:layout_marginBottom="734dp"
        android:onClick="keranjang"
        android:src="@drawable/keranjang"
        tools:ignore="OnClick" />


    <ImageView
        android:id="@+id/masuklogin"
        android:layout_width="70dp"
        android:layout_height="56dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="3dp"
        android:layout_marginBottom="734dp"
        android:src="@drawable/orang2"
        tools:ignore="OnClick" />

    <ViewFlipper
        android:id="@+id/flipper"
        android:layout_width="match_parent"
        android:layout_height="216dp"
        android:layout_below="@id/searchView"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="193dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="442dp" />

    <androidx.appcompat.widget.SearchView
        android:id="@+id/searchView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:layout_marginBottom="663dp"
        android:background="@drawable/ser"
        app:iconifiedByDefault="false"
        app:queryHint="Cari disini"

        />

    <ImageView
        android:id="@+id/fitnes"
        android:layout_width="155dp"
        android:layout_height="209dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="419dp"
        android:layout_marginEnd="208dp"
        android:layout_marginBottom="223dp"
        android:src="@drawable/fitt" />

    <ImageView
        android:id="@+id/menu"
        android:layout_width="155dp"
        android:layout_height="209dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="215dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="222dp"
        android:src="@drawable/menu" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycleView"
        android:layout_width="336dp"
        android:layout_height="168dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="24dp" />




</RelativeLayout>